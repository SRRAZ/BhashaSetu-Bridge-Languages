<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- Regular font -->
    <font
        android:font="@font/noto_sans_devanagari_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/noto_sans_devanagari_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- Bold font -->
    <font
        android:font="@font/noto_sans_devanagari_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/noto_sans_devanagari_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>